pipeline{
    agent any
    stages{
        stage("GIT checkout"){
            steps{
               git credentialsId: 'javahome2', url: 'https://github.com/sirikdevops/myweb.git'
                
            }
        }
        stage("Maven Build"){
            steps{
               withMaven(maven: 'mvn') {
               sh "mvn clean package"
              }
                
            }
         }
    }
}
